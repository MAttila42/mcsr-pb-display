{
  "manifest_version": 3,
  "name": "MCSR PB Display",
  "version": "0.1.0",
  "description": "My Chrome Extension",
  "action": {
    "default_title": "MCSR PB Display",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "identity",
    "cookies"
  ],
  "host_permissions": [
    "https://*.twitch.tv/"
  ],
  "icons": {
    "48": "icon.png",
    "96": "icon.png",
    "128": "icon.png"
  }
}
